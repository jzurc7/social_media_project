library(class)
set.seed(123)
train_indices <- sample(nrow(Ionosphere), 0.7 * nrow(Ionosphere), replace = FALSE)
train.data <- Ionosphere[train_indices, ]
test.data <- Ionosphere[-train_indices, ]
knn.pred <- knn(
train = train.data[,-ncol(train.data)],
test = test.data[,-ncol(test.data)],
cl = train.data$Class,
k = 3
)
conf_matrix <- table(knn.pred, test.data$Class)
print(conf_matrix)
accuracy <- mean(knn.pred == test.data$Class)
cat("Accuracy:", accuracy, "\n")
knn.pred5 <- knn(
train = train.data[,-ncol(train.data)],
test = test.data[,-ncol(test.data)],
cl = train.data$Class,
k = 5
)
conf_matrix5 <- table(knn.pred5, test.data$Class)
print(conf_matrix5)
accuracy5 <- mean(knn.pred5 == test.data$Class)
cat("Accuracy for K=5:", accuracy5, "\n")
knn.pred7 <- knn(
train = train.data[,-ncol(train.data)],
test = test.data[,-ncol(test.data)],
